  },
  "properties": {
    "description": {
      "type": "string",
      "required": true
    },
    "place": {
      "type": "geopoint"
    },
    "datetime": {
      "type": "date",
      "required": true
    },
    "mediaIds": {
      "type": [
        "object"
      ],
      "default": []
    },
    "userId": {
      "type": "string",
      "default": null
    },
    "taskId": {
      "type": "string",
      "default": null
    },
    "rating": {
      "type": "object",
      "default": {
        "up": {
          "count": 0,
          "users": []
        },
        "down": {
          "count": 0,
          "users": []
        }
      }
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    },
    "task": {
      "type": "belongsTo",
      "model": "task",
      "foreignKey": "taskId"
